<h1> List of Movies </h1>
    <table>
      <tr>
        <th>Movie Title</th>
        <th>Year released</th>
        <th>Rating</th>
        <th>Genre</th>
        <th>Studio</th>
      <tr/>

      <tr>
        <% @movies.each do |movie| %>
        <td><a href="/movies/<%= movie["movie_id"] %>"><%= movie["movie_title"] %></a></td>
        <td><%= movie["release_date"] %></td>
        <td><%= movie["movie_rating"] %></td>
        <td><%= movie["genre"] %></td>
        <td><%= movie["studio_name"] %></td>
      </tr>
          <% end %>
      </table>
